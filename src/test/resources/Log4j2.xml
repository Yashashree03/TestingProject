<?xml version="1.0" encoding="UTF-8"?>
<Configuration strict="true">
<Appenders>

<Appender type="Console" name="con">
<Layout type="PatternLayout" pattern="%d{HH:mm:ss:SSS} [%t] %-7level %logger{36} -%m%n"></Layout>
</Appender>

<Appender type="file" name="fi" filename="C:\Users\DELL\OneDrive\Desktop\Yashashree\Logger.log">
<Layout type="PatternLayout" pattern="%d{HH:mm:ss:SSS} [%t] %-7level %logger{36} -%m%n"></Layout>
</Appender>

</Appenders>

<loggers>
<Root level="all">
<AppenderRef ref="con"></AppenderRef>
</Root>
</loggers>

</Configuration>